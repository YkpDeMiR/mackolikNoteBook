   "source": [
    "from selenium import webdriver\n",
    "from selenium.webdriver.common.by import By\n",
    "from selenium.webdriver.firefox.service import Service as FirefoxService\n",
    "import time\n",
    "\n",
    "\n",
    "# Geckodriver yolu\n",
    "geckodriver_path = \"./geckodriver\"\n",
    "\n",
    "# Firefox service nesnesini oluştur\n",
    "service = FirefoxService(executable_path=geckodriver_path)\n",
    "\n",
    "\n",
    "# Firefox tarayıcısını service ile başlat\n",
    "driver = webdriver.Firefox(service=service)\n",
    "# Hedef web sayfasına git\n",
    "driver.get(\"https://www.mackolik.com/futbol/canli-sonuclar\")\n",
    "time.sleep(5)\n",
    "# Belirli bir class name'e sahip öğeyi bul\n",
    "# element = driver.find_element(By.XPATH, \"/html/body/div[1]/div/div/div/div/div[2]/div/button[2]/span\")\n",
    "# element.click()\n",
    "element2 = driver.find_element(By.CSS_SELECTOR, \"#widget-livescore-match-row-a93uwolyri53gex23mseyfoyc > div:nth-child(1) > div:nth-child(4) > div:nth-child(1) > a:nth-child(2)\")\n",
    "element2.click()\n",
    "\n",
    "hometeam = driver.find_element(By.XPATH,\"/html/body/div[5]/div[1]/div[1]/div/div[1]/a[1]\")\n",
    "hometeam.text()"
   ]
